      }
    },
    {
      "cell_type": "code",
      "source": [
        "def persistence(n):\n",
        "    count = 0\n",
        "    while n >= 10:\n",
        "        n = eval('*'.join(str(n)))\n",
        "        count += 1\n",
        "    return count\n\n",
        "# Тест\n",
        "print(persistence(39))  # Ожидается 3"
      ],
